{
  "name": "j1-nbi-core",
  "version": "1.0.0",
  "description": "Library to allow public interactive Jupyter notebooks",
  "main": "lib/index.bundle.js",
  "module": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no tests available\" && exit 1",
    "build": "webpack --mode production --devtool source-map",
    "watch": "webpack --watch",
    "serve": "webpack-dev-server --open",
    "stats": "webpack --profile --json > stats.json",
    "stats-prod": "webpack --profile --json --mode production > stats-prod.json",
    "analyze": "webpack-bundle-analyzer -p 9999 stats.json",
    "analyze-prod": "webpack-bundle-analyzer -p 9999 stats-prod.json",
    "clean": "rimraf lib/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jekyll-one/j1-nbi.git"
  },
  "keywords": [
    "jupyter",
    "notebook",
    "html",
    "interactive",
    "widgets"
  ],
  "author": "Juergen Adams",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jekyll-one/j1-nbi/issues"
  },
  "homepage": "https://github.com/jekyll-one/j1-nbi#readme",
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.8",
    "@jupyter-widgets/controls": "^1.1.5",
    "@jupyter-widgets/html-manager": "^0.11.9",
    "@jupyter-widgets/output": "^1.0.15",
    "@jupyterlab/services": "^1.1.2",
    "bqplot": "0.4.0",
    "font-awesome": "^4.7.0",
    "lodash.once": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@types/requirejs": "^2.1.28",
    "babel-core": "^6.26.0",
    "babel-loader": "8.0.0-beta.2",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "fs-extra": "^0.30.0",
    "json-loader": "^0.5.4",
    "lerna": "^3.8.0",
    "postcss": "^6.0.2",
    "prettier": "^1.6.1",
    "schema-utils": "^0.4.3",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.1",
    "lodash.debounce": "^4.0.8",
    "rimraf": "^2.6.2"
  },
  "gitHead": "9f00f654748b85702463b60a98751943c0107486"
}
